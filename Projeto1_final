
import cx_Oracle

def insertDados(nome_dados, idade_dados, sexo_dados, peso_dados, altura_dados, imc_dados, gasto_dados, sum_dados, data_dados):
    sql = ('insert into dados_imc(nome_dados, idade_dados, sexo_dados, peso_dados, altura_dados, imc_dados, gasto_dados, sum_dados, data_dados) '
        'values(:nome_dados,:idade_dados,:sexo_dados,:peso_dados,:altura_dados,:imc_dados,:gasto_dados,:sum_dados,:data_dados)')

    try:
        connection = cx_Oracle.connect(user="BDESI12", password="Kjscr10",dsn="ceatudb02/xe")
        cursor = connection.cursor()
        cursor.execute(sql, [nome_dados, idade_dados, sexo_dados, peso_dados, altura_dados, imc_dados, gasto_dados, sum_dados, data_dados])
        connection.commit()

    except cx_Oracle.Error as error:
        print('Error occurred:')
        print(error)
